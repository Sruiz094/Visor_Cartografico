psComponent/marcador.ts","name":null,"original":{"line":44,"column":5},"generated":{"line":41,"column":3}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":44,"column":6},"generated":{"line":41,"column":4}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":45,"column":0},"generated":{"line":42,"column":0}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":0}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":7}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":8}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":19}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":22}},{"source":"mapsComponent/marcador.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":43,"column":33}}],"sources":{"mapsComponent/marcador.ts":"import $ from \"jquery\";\nimport { Map, Marker } from \"leaflet\";\n\nexport function coordenadas(mymap: Map) {\n    let marker: Marker | null = null;\n\n    // Manejador de evento para el botón search-btn\n    $('#search-btn').click(function () {\n        console.log(\"Botón de búsqueda clickeado\"); \n        const latlng = $('#search').val() as string;\n        console.log(\"Coordenadas ingresadas:\", latlng); \n        const latlngArr = latlng.split(',');\n        const lat = parseFloat(latlngArr[0]);\n        const lng = parseFloat(latlngArr[1]);\n\n        // Si el marcador no existe, créalo y añádelo al mapa\n        if (!marker) {\n            console.log(\"Creando nuevo marcador\"); \n            marker = new Marker([lat, lng]).addTo(mymap);\n        } else {\n            // Si el marcador ya existe, actualiza su posición\n            console.log(\"Actualizando posición del marcador\"); \n            marker.setLatLng([lat, lng]);\n        }\n\n        // Establecer la vista del mapa con las coordenadas ingresadas por el usuario\n        mymap.setView([lat, lng], 18);\n    });\n\n    // Manejador de evento para el campo de entrada #search\n    $('#search').on('input', function() {\n        console.log(\"Campo de búsqueda cambiado\"); \n        const latlng = $('#search').val() as string;\n\n        // Si las coordenadas están borradas, eliminar el marcador del mapa\n        if (!latlng.trim() && marker) {\n            console.log(\"Borrando marcador del mapa\"); \n            mymap.removeLayer(marker);\n            marker = null;\n            \n            // Centrar la vista del mapa a la posición inicial\n            mymap.setView([2.581672, -75.523207], 9);\n        }\n    });\n}\n"},"lineCount":null}},"error":null,"hash":"49a5e4f1622cb170a5e621cd41169637","cacheData":{"env":{}}}